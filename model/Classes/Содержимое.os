Перем data Экспорт;

// Основной контент.
//
Перем errors Экспорт;

Функция Структура() Экспорт

	Структура = Новый Структура();

	Если errors <> Неопределено Тогда
		Структура.Вставить("errors", errors);
	Иначе
		Структура.Вставить("data", data);
	КонецЕсли;

	Возврат Структура;

КонецФункции